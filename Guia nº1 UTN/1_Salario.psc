Algoritmo sin_titulo
	Definir identificacion, horas COMO ENTERO;
	Definir salario, total COMO REAL;
	Escribir "Ingrese la identificación del empleado";
	Leer identificacion;
	Escribir "Ingrese el salario por hora del empleado";
	Leer salario;
	Escribir "Ingrese la cantidad de horas que trabajo";
	Leer horas;
	total = salario * horas;
	Escribir "Identificación del empleado ", identificacion;
	Escribir "El empleado ganó ", total, " pesos este mes";

FinAlgoritmo
